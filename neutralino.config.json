{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "brandmeister.monitor",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": false,
    "writeToLogFile": false
  },
  "storageLocation": "system",
  "nativeAllowList": [
    "app.*",
    "os.*",
    "storage.*",
    "debug.log"
  ],
  "globalVariables": {
  },
  "modes": {
    "window": {
      "title": "Brandmeister Monitor",
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/icons/icon.png",
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    },
    "browser": {
      "globalVariables": {
        "TEST_MODE": "true"
      },
      "nativeBlockList": []
    }
  },
  "cli": {
    "binaryName": "brandmeister-monitor",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "6.3.0",
    "clientVersion": "6.3.0"
  }
}